<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ItemStyle.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ItemStyle.Pages"
    Shell.FlyoutBehavior="Locked"
    Title="ItemStyle">

    <ShellContent
        Title="TriggerCheck"
        ContentTemplate="{DataTemplate local:TriggerCheckPage}"
        Route="TriggerCheckPage" />
    
    <ShellContent
        Title="VsmCheck"
        ContentTemplate="{DataTemplate local:VSMCheckPage}"
        Route="VSMCheckPage" />
    
    <ShellContent
        Title="BindingCheck"
        ContentTemplate="{DataTemplate local:BindingCheckPage}"
        Route="BindingCheckPage" />

</Shell>
